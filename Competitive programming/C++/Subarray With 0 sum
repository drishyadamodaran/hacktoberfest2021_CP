
#include <bits/stdc++.h>
using namespace std;

bool subArrayExists(int arr[], int n)
{
	unordered_set<int> sumSet;

	
	int sum = 0;
	for (int i = 0; i < n; i++)
	{
		sum += arr[i];

		
		if (sum == 0
			|| sumSet.find(sum)
			!= sumSet.end())
			return true;

		sumSet.insert(sum);
	}
	return false;
}

int main()
{
	int arr[] = { -3, 2, -1, -1, 6 };
	int n = sizeof(arr) / sizeof(arr[0]);
	if (subArrayExists(arr, n))
		cout << "Found a subarray";
	else
		cout << "No Such Sub Array ";
	return 0;
}
